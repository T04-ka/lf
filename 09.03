def DICTing(objects:list,inp_attr:list):
    for i in inp_attr:
        for j in range(len(objects)):
            atr = objects[j].__dict__
    return atr

def sort1(atr, attr):
    out = [[] * 1 for i in range (len(attr))]
    t = 0
    for i in attr:
        out[t].append(atr[i])
        t += 1
    return(out)
