def DICTing(objects:list,inp_attr:list):
    for i in inp_attr:
        for j in range(len(objects)):
            atr = objects[j].__dict__
    return atr

def sort(atr,attr):
    bol = True
    for i in attr:
        if bol:
            prev = atr.get(i)
            bol = False
            continue
        now = atr.get(i)
        if now > prev:
            atr[i-1],atr[i] = atr[i],atr[i-1]
        prev = atr.get(i)
    return(atr)
            
