class  Stack:
    data = []
    def __init__(self):
        pass
    def Append(val):
        A = [val]
        Stack.data = A + Stack.data
        return val
        
    def POP():
        aval = Stack.data[-1]
        Stack.data.pop(self)
        return aval
        
    def Peek(self):
        anlav = Stack.data.peak()   

        return anval
